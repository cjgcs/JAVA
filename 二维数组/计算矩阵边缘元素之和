import java.util.Scanner;

public class C计算矩阵边缘元素之和 {

		public static void main(String[] args) {
			Scanner kb=new Scanner(System.in);
			int n,m;
			int sum = 0;
			n=kb.nextInt();
			m=kb.nextInt();
			int [][] a=new int[n][m];
			for(int i = 0;i<n;i++) {
				for(int j = 0;j<m;j++) {
					a[i][j]=kb.nextInt();
					if(i==0|j==0|i==n-1|j==m-1){
					sum=sum+a[i][j];
					
					}
				}
			}
			System.out.println(sum);
	}

}
